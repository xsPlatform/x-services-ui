{
  "version": "0.0.2",
  "name": "x-services-ui",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack --watch --progress --mode development --config webpack.dev.js",
    "build:prod": "webpack --mode production --config webpack.prod.js",
    "build:dev": "webpack --mode development --config webpack.dev.js ",
    "build:npm": "babel src -d dist",
    "build:npm:watch": "babel src -w -d dist",
    "build:storybook": "build-storybook -c .storybook -o .out",
    "test": "cross-env NODE_ENV=test jest ./src -u",
    "test:watch": "cross-env NODE_ENV=test jest ./src --watch",
    "test:coverage": "cross-env NODE_ENV=test jest ./src --coverage",
    "test:coverage:watch": "cross-env NODE_ENV=test jest ./src --watch --coverage",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "eslint --ext .jsx,.js src/ --fix",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/*.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "check:consistency": "npm run lint && npm run prettier",
    "prepare": "npm run build:npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:consistency"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
